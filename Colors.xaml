	<Color x:Key="ColorCabBlue3">#618898</Color>
	<!-- 97, 136, 152 -->
	<Color x:Key="ColorCabBlue4">#80a0ad</Color>
	<!-- 128, 160, 173 -->
	<Color x:Key="ColorCabBlue5">#a0b8c1</Color>
	<!-- 160, 184, 193 -->
	<Color x:Key="ColorCabBlue6">#c0cfd6</Color>
	<!-- 192, 207, 214 -->
	<Color x:Key="ColorCabBlue7">#dfe7eb</Color>
	<!-- 223, 231, 235 -->

	<Color x:Key="ColorCabOrange">#ff8500</Color>
	<!-- 255, 133, 0 -->
	<Color x:Key="ColorCabOrange1">#ff9420</Color>
	<!-- 255, 148, 32 -->
